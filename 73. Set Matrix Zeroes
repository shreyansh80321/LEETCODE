class Solution {
public:
    void setZeroes(vector<vector<int>>& matrix) {
        vector<int> v1;
        int rows=matrix.size();
        int cols=matrix[0].size();
        cout<<rows<<" "<<cols<<endl;
        // for(int i=0;i<cols;i++)
        // {
        //     if(matrix[0][i]==0)
        //     {
        //         for(int j=0;j<cols;j++)
        //         {
        //             matrix[0][j]=0;
        //         }
        //     }
        // }
        // for(int i=0;i<rows;i++)
        // {
        //     if(matrix[i][0]==0)
        //     {
        //         for(int j=0;j<rows;j++)
        //         {
        //             matrix[j][0]=0;
        //         }
        //     }
        // }
          for(int i=0;i<matrix.size();i++)
        {
            for(int j=0;j<matrix[i].size();j++)
            {
               if(matrix[i][j]==0)
               {
                v1.push_back(i);
                v1.push_back(j);
               }
            }
        }
            for(int i=1;i<v1.size();i+=2)
        {
            cout<<v1[i]<<endl;
            for(int j=0;j<rows;j++)
            {
                // cout<<matrix[][]<<endl;
            
                matrix[j][v1[i]]=0;
            }
        }
        for(int i=0;i<v1.size();i+=2)
        {
            for(int j=0;j<cols;j++)
            {
                matrix[v1[i]][j]=0;
            }
        }
    
        // for(int i=0;i<matrix.size();i++)
        // {
        //     for(int j=0;j<matrix[i].size();j++)
        //     {
        //         if(matrix[i][j]==0)
        //         {
        //             for(int k=0;k<rows;k++)
        //             {
        //                 cout<<matrix[k][j]<<" ";
        //                 matrix[k][j]=0;
        //                 // matrix[k][j]=0;
        //             }
        //             cout<<endl;
        //             for(int k=0;k<cols;k++)
        //             {
        //                 cout<<matrix[i][k]<<" ";
        //                 matrix[i][k]=0;
        //             }
        //             return;
        //             cout<<endl;

        //         }

        //     }
        // }
      
        // for(int i=0;i<v1.size();i++)
        // {
        //     cout<<v1[i]<<" ";
        // }
        // cout<<endl;
        // return mat
    }
};
