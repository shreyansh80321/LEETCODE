class Solution {
public:
    vector<int> searchRange(vector<int>& nums, int target) {
        vector<int> v1;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]==target)
            {
                v1.push_back(i);
            }
        }
        if(v1.size()==0)
        {
            v1.push_back(-1);
            v1.push_back(-1);
        }
        if(v1.size()==1)
        {
            int val=v1[0];
            v1.push_back(val);
        }
        if(v1.size()>2)
        {
            vector<int> ans;
            ans.push_back(v1[0]);
            ans.push_back(v1[v1.size()-1]);
            return ans;
        }
        return v1;
    }
};
