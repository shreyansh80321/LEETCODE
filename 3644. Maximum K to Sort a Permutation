class Solution {
public:
    int sortPermutation(vector<int>& nums) {
        vector<int> v1;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]!=i)
            {
              v1.push_back(nums[i]);
            }
        }
        if(v1.size()==0)
        {
            return 0;
        }
        else
        {
        int j=v1[0]&v1[1];
        for(int i=2;i<v1.size();i++)
        {
          j&=v1[i];
        }
        return j;
        }
    }
};
