class Solution {
public:
   void setSubsets(vector<int>& nums, vector<vector<int>>& v1,int k,vector<int> ans)
    {
        if(k==nums.size())
        {
            v1.push_back({ans});
            return;
        }
        ans.push_back(nums[k]);
        setSubsets(nums,v1,k+1,ans);
        ans.pop_back();
        int idx=k+1;
        while(idx<nums.size()&&nums[idx]==nums[idx-1])
        {
            idx++;
        }
        setSubsets(nums,v1,idx,ans);

    }
    vector<vector<int>> subsetsWithDup(vector<int>& nums) {
        sort(nums.begin(),nums.end());
        // int k=0;
        vector<vector<int>> v1;
        vector<int> ans;
        // int k=0;
        setSubsets(nums,v1,0,ans);
        return v1;
        
    }
};
