class Solution {
public:
    int maxDepth(string s) {
        stack<char> st;
        int cnt1=0;
        int cnt2=0;
        for(int i=0;i<s.length();i++)
        {
            if(s[i]=='(') cnt1++;
            if(s[i]==')') cnt1--;
            cnt2=max(cnt2,cnt1);
        }
        return cnt2;
    }
};
