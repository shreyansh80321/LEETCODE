class Solution {
public:
    int findMaxConsecutiveOnes(vector<int>& nums) {
        int cnt=0;
        int maxi=INT_MIN;
        if(nums.size()==1&&nums[0]==1)
        {
            return nums[0];
        }
        bool flag=false;
        for(int i=0;i<nums.size();i++)
        {
            if(nums[i]==1)
            {
                flag=true;
            }
        }
        if(flag==false)
        {
            return 0;
        }
        else
        {

        
        for(int i=0;i<nums.size()-1;i++)
        {
            if(nums[i]==1&&nums[i+1]==1)
            {
                cnt++;
            }
            else{
                cnt=0;
            }
            maxi=max(maxi,cnt);
        }
        return maxi+1;
        }
    }
};
