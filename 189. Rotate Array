class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        vector<int> v1(nums.size(),0);
        for(int i=0;i<nums.size();i++)
        {
            v1[(i+k)%nums.size()]=nums[i];
        }
        for(int i=0;i<v1.size();i++)
        {
            nums[i]=v1[i];
        }
        cout<<endl;
    }
};
