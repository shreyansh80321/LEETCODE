class Solution {
public:
    bool isIsomorphic(string s, string t) {
        unordered_map<char,char> v1;
        unordered_map<char,char> v2;
        bool valid=true;
        for(int i=0;i<s.size();i++)
        {
            char c1=s[i];
            char c2=t[i];
            if(v1.count(c1)&&v1[c1]!=c2)
            {
                    valid=false;
                    break;
            }
            if(v2.count(c2)&&v2[c2]!=c1)
            {
                valid=false;
                break;
            }
            else
            {
                v1[c1]=c2;
                v2[c2]=c1;
            }
        }
         return valid;
    }
};
